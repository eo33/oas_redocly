{
  "name": "@redocly/portal-plugin-mock-server",
  "version": "0.3.59",
  "description": "Mock Server plugin for @redocly/portal",
  "author": "team@redocly.com",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "main": "lib/plugin.js",
  "exports": {
    ".": "./lib/plugin.js",
    "./plugin.js": "./lib/plugin.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@redocly/mock-server": "0.2.3",
    "@redocly/openapi-core": "1.25.15",
    "@redocly/openapi-docs": "3.5.7",
    "@redocly/config": "0.18.0"
  },
  "devDependencies": {
    "@types/node": "18.19.3",
    "esbuild-minify-dir": "^0.0.1",
    "rimraf": "5.0.7"
  },
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc -p tsconfig.build.json",
    "minify": "esbuild-minify-dir ./lib",
    "build": "npm run clean && npm run compile && npm run minify",
    "watch": "tsc -w -p tsconfig.build.json"
  }
}