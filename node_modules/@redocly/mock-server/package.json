{
  "name": "@redocly/mock-server",
  "version": "0.2.3",
  "description": "Redocly Mock server",
  "author": "team@redocly.com",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/bin.js",
  "dependencies": {
    "@redocly/ajv": "8.11.2",
    "@redocly/openapi-core": "1.25.15",
    "ajv": "8.17.1",
    "ajv-formats": "^2.1.1",
    "js-yaml": "4.1.0",
    "openapi-sampler": "1.6.1",
    "punycode": "2.3.0",
    "swagger2openapi": "^7.0.8",
    "ts-node": "^10.9.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "@types/jest-when": "3.5.5",
    "@types/js-yaml": "4.0.9",
    "@types/node": "17.0.45",
    "@types/yargs": "17.0.32",
    "axios": "1.7.4",
    "esbuild": "0.17.15",
    "jest": "28.1.3",
    "jest-when": "3.6.0",
    "openapi-types": "10.0.0",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.7",
    "ts-jest": "28.0.8",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.2"
  },
  "scripts": {
    "start": "npx ts-node-dev -r tsconfig-paths/register --respawn src/cli/cli.ts mock ./tests/fixtures/large-openapi.json",
    "ts:check": "npx tsc --noEmit --skipLibCheck",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "npx jest ./src",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:integration": "npx jest ./tests --forceExit",
    "test:integration:coverage": "npm run test:integration -- --coverage",
    "build:js": "node ./esbuild.js",
    "build": "npx rimraf ./dist && npm run build:js"
  }
}